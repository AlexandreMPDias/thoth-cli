{
	"name": "thoth-cli",
	"version": "0.0.0",
	"description": "Command Line Helper Lib",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist",
	"scripts": {
		"prebuild": "echo rm -rf ./dist",
		"build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
		"dev": "yarn build && tsc-watch --onSuccess \"yarn fixPaths\"",
		"fixPaths": "tscpaths -p tsconfig.json -s ./src -o ./dist",
		"test": "cross-env APP_ENV=testing jest --passWithNoTests --silent --noStackTrace --runInBand",
		"test:verbose": "cross-env APP_ENV=testing jest --passWithNoTests --runInBand",
		"test:unit": "cross-env TEST_SCOPE=unit yarn test:verbose --watch",
		"test:int": "cross-env TEST_SCOPE=integration yarn test:verbose --watch",
		"test:staged": "cross-env TEST_SCOPE=staged yarn test --watch",
		"test:ci": "cross-env TEST_SCOPE=ci yarn test --coverage",
		"test:deploy": "cross-env TEST_SCOPE=ci yarn test",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "echo tslint -p tsconfig.json",
		"prepublishOnly": "npm run test:deploy && npm run lint",
		"preversion": "npm run lint",
		"version": "yarn build && npm run format && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexandreMPDias/thoth-cli.git"
	},
	"keywords": [
		"cli"
	],
	"author": "AlexandreMPDias",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/AlexandreMPDias/thoth-cli/issues"
	},
	"homepage": "https://github.com/AlexandreMPDias/thoth-cli#readme",
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.22",
		"@types/ramda": "^0.27.38",
		"@types/yargs": "^16.0.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"cross-env": "^7.0.3",
		"jest": "^26.6.3",
		"jest-extended": "^0.11.5",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.0",
		"ts-node": "^9.1.1",
		"tsc-watch": "^4.2.9",
		"tscpaths": "^0.0.9",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"clear": "^0.1.0",
		"commander": "^7.1.0",
		"ramda": "^0.27.1",
		"yargs": "^16.2.0"
	}
}
